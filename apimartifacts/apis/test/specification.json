          "multipart/form-data"
        ],
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/ApiResponse"
            },
            "examples": {
              "application/json": {
                "code": 0,
                "type": "string",
                "message": "string"
              }
            }
          }
        }
      }
    },
    "/pet": {
      "put": {
        "operationId": "updatePet",
        "summary": "Update an existing pet",
        "tags": [
          "pet"
        ],
        "parameters": [
