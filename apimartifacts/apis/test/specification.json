    "/store/order/{orderId}": {
      "get": {
        "description": "For valid response try integer IDs with value >= 1 and <= 10. Other values will generated exceptions",
        "operationId": "getOrderById",
        "summary": "Find purchase order by ID",
        "tags": [
          "store"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "description": "Format - int64. ID of pet that needs to be fetched",
            "required": true,
            "type": "integer",
            "format": "int64",
            "maximum": 10,
            "minimum": 1
          }
        ],
        "produces": [
          "application/json",
          "application/xml"
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Order"
            },
            "examples": {
              "application/json": {
                "id": 0,
                "petId": 0,
                "quantity": 0,
                "shipDate": "string",
                "status": "placed",
                "complete": true
              },
              "application/xml": "<Order>\r\n  <id>0</id>\r\n  <petId>0</petId>\r\n  <quantity>0</quantity>\r\n  <shipDate>string</shipDate>\r\n  <status>placed</status>\r\n  <complete>true</complete>\r\n</Order>"
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Order not found"
          }
        }
      },
      "delete": {
        "description": "For valid response try integer IDs with positive integer value. Negative or non-integer values will generate API errors",
        "operationId": "deleteOrder",
        "summary": "Delete purchase order by ID",
        "tags": [
          "store"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "description": "Format - int64. ID of the order that needs to be deleted",
            "required": true,
            "type": "integer",
            "format": "int64",
            "minimum": 1
          }
        ],
        "produces": [
          "application/json",
          "application/xml"
        ],
        "responses": {
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Order not found"
          }
        }
      }
    },
